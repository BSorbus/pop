        </div> <!-- /id="collapse_family_print" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-11 col-sm-offset-1 -->
  </div> <!-- /row -->

  <%= render partial: "documents/documents_in_panel", locals: {documentable: @individual} %>

</div> <!-- /class="panel-group" id="myaccordion" -->