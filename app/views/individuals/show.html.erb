<br>
<div class="panel-group" id="myaccordion">

  <!-- Dane -->
  <div class="row">
    <div class="col-sm-12">
      <div class="panel panel-individual">
        <div class="panel-heading">
          <div class="panel-title">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_data" >
              <span class="glyphicon glyphicon-user">  <%= @individual.fullname %></span>  
              <i class="indicator glyphicon glyphicon-chevron-down  pull-right"></i>
            </a>
          </div>
        </div>
        <div id="collapse_data" class="panel-collapse collapse in"> <!-- id="collapse_data" -->
          <div class="panel-body">
            <div class="col-sm-11 col-sm-offset-1">

              <%= render 'form_show', input_disabled: true %>

            </div> <!--/col-sm-11 col-sm-offset-1-->    
          </div>
        </div> <!-- /id="collapse_data" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-12-->
  </div> <!-- /row -->


  <!-- Polisy NNW -->
  <div class="row">
    <div class="col-sm-11 col-sm-offset-1">
      <div class="panel panel-insurance">
        <div class="panel-heading">
          <div class="panel-title">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_insurances" >
              <span class="glyphicon glyphicon-file"> ...występuje w polisach NNW</span>    
              <i class="indicator glyphicon glyphicon-chevron-right  pull-right"></i>
            </a>
          </div>
        </div>
        <div id="collapse_insurances" class="panel-collapse collapse"> <!-- id="collapse_insurances" -->
          <div class="panel-body">
            <div class="col-sm-12">



              <div class="panel panel-default">
                <div class="panel-heading">
                  <div class="panel-title">
                    <a class="accordion-toggle" data-toggle="collapse" href="#collapse_nnw_as_insured" >
                      <span class="glyphicon glyphicon-gift"> ...jako Ubezpieczony</span>    
                      <i class="indicator glyphicon glyphicon-chevron-right  pull-right"></i>
                    </a>
                  </div>
                </div>
                <div id="collapse_nnw_as_insured" class="panel-collapse collapse"> <!-- id="collapse_nnw_as_insured" -->
                  <div class="panel-body">
                    <div class="col-sm-12">

                      <%= render 'nnw_as_insured' %>

                    </div> <!--/class="col-sm-12" -->
                  </div> <!-- /panel-body -->
                </div> <!-- /id="collapse_nnw_as_insured" -->
              </div> <!-- /panel -->


              <div class="panel panel-default">
                <div class="panel-heading">
                  <div class="panel-title">
                    <a class="accordion-toggle" data-toggle="collapse" href="#collapse_nnw_as_payer" >
                      <span class="glyphicon glyphicon-piggy-bank"> ...jako Płatnik</span>    
                      <i class="indicator glyphicon glyphicon-chevron-right  pull-right"></i>
                    </a>
                  </div>
                </div>
                <div id="collapse_nnw_as_payer" class="panel-collapse collapse"> <!-- id="collapse_nnw_as_payer" -->
                  <div class="panel-body">
                    <div class="col-sm-12">

                      <%= render 'nnw_as_payer' %>

                    </div> <!--/class="col-sm-12" -->
                  </div> <!-- /panel-body -->
                </div> <!-- /id="collapse_nnw_as_payer" -->
              </div> <!-- /panel -->



            </div> <!--/class="col-sm-12" -->
          </div> <!-- /panel-body -->
        </div> <!-- /id="collapse_insurances" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-11 col-sm-offset-1 -->
  </div> <!-- /row -->


  <!-- Wydruki NNW -->
  <div class="row">
    <div class="col-sm-10 col-sm-offset-2">
      <div class="panel panel-print">
        <div class="panel-heading">
          <div class="panel-title">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_nnw_print" >
