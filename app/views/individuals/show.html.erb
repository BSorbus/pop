                  </div> <!-- /panel-body -->
                </div> <!-- /id="collapse_family_as_payer" -->
              </div> <!-- /panel -->



            </div> <!--/class="col-sm-12" -->
          </div> <!-- /panel-body -->
        </div> <!-- /id="collapse_families" -->
      </div> <!-- /panel -->
    </div> <!-- /col-sm-11 col-sm-offset-1 -->
  </div> <!-- /row -->


  <!-- Wydruki Rodzina-->
  <div class="row">
    <div class="col-sm-10 col-sm-offset-2">
      <div class="panel panel-print">
        <div class="panel-heading">
          <div class="panel-title">
            <a class="accordion-toggle" data-toggle="collapse" data-parent="#myaccordion" href="#collapse_family_print" >
